{
    "name": "janusnic/latacomm",
    "description": "Laravel comments system",
    "keywords": [
        "janusnic",
        "laracomm"
    ],
    "homepage": "https://github.com/janusnic/laracomm.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Janus Nicon",
            "email": "janusnic@gmail.com",
            "homepage": "https://janusnic.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Janusnic\\Laracomm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Janusnic\\Laracomm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
